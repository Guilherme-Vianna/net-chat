{
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_CONNECTION}"
  },
  "Jwt": {
    "Key":"${JWT_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:4040"
      }
    }
  },
  "AllowedHosts": "*"
}
